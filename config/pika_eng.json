  "value_types": {
    "integer": {
      "key": 0,
      "name": "Integer (int)",
      "validate": "[0-9]+",
      "remove": " ",
      "format": "{}",
      "hint": "Example: 100"
    },
    "bool": {
      "key": 1,
      "name": "Boolean (bool)",
      "validate": "(true|false)",
      "remove": " ",
      "format": "{}",
      "hint": "Example: true or false"
    },
    "float": {
      "key": 2,
      "name": "Float (float)",
      "validate": "[0-9]+.?[0-9]*",
      "remove": " ",
      "format": "{}",
      "hint": "Example: 1.0"
    },
    "list": {
      "key": 3,
      "name": "List (list)",
      "validate": " *\\[? *([0-9]+( *, *[0-9]+)*)? *\\]? *",
      "remove": "[ \\[\\]]",
      "format": "[{}]",
      "hint": "Example: 1, 2 or [1, 2]"
    },
    "string": {
      "key": 4,
      "name": "String (str)",
      "validate": "(\".*\"|'.*'|[^\"']*)",
      "remove": "(^\"|^'|\"$|'$)",
      "format": "\"{}\"",
      "hint": "Example: \"abc\", 'abc' or abc"
    },
    "error": {
      "key": 5,
      "name": "An error will occur when running",
      "validate": "",
      "remove": null,
      "format": "Runtime error",
      "hint": "No value is needed"
